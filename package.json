{
  "name": "k6-arch-example",
  "version": "0.0.1",
  "description": "Projeto para exemplo de arquitetura para testes de performance usando o k6",
  "main": "index.js",
  "scripts": {
    "FullFlowLoadTest": "k6 run -e env=PRD src/simulations/FullFlowLoad.test.js",
    "FullFlowStressTest": "k6 run -e env=PRD src/simulations/FullFlowStress.test.js",
    "docker:FullFlowLoadTest": "docker-compose run k6 run -e env=PRD /src/simulations/FullFlowLoad.test.js",
    "docker:FullFlowStressTest": "docker-compose run k6 run -e env=PRD /src/simulations/FullFlowStress.test.js",
    "docker:up": "docker-compose up -d influxdb grafana",
    "docker:down": "docker-compose down -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianoalves/k6-arch-example.git"
  },
  "author": "Maximiliano Alves da Cruz",
